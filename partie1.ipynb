   "source": [
    "def remove_duplicate(list1,list2):\n",
    "  common_values=list(set(list1)&set(list2))\n",
    "  for i in common_values:\n",
    "    list1.remove(i)\n",
    "    list2.remove(i)\n",
    "  return list1+list2\n",
    "  \n",
    "list1=list(input('first person: '))\n",
    "list2=list(input('second person: '))\n",
    "print(remove_duplicate(list1,list2))"
   ]
  }
